#include <stdlib.h>
#include <iostream>
using namespace std;

namespace  one {
    int i;

}


namespace two{
    int j;

}

int main ()
{
    one::i=10;
    two::j=20;
    using namespace one;
    using namespace two;
cout <<" first namespace "<<i<<endl;
cout <<" second namespace "<<j;
    return 0;
}
